{
	"name": "fuck twitter",
	"description": "ligma",
	"manifest_version": 2,
	"version": "0",
	
	"permissions": [
		"cookies",
		"webRequest",
		"webRequestBlocking",
		"https://.twitter.com/*",
		"https://twitter.com/*"
	],
	"content_scripts": [{
		"matches": ["https://twitter.com/*"],
		"run_at": "document_start",
		"js": ["inject.js"]
	}],
	"web_accessible_resources": ["assets/*"],
	"background": {
		"scripts": ["cookies.js"]
	}
}
